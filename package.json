{
  "name": "travel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "build:native": "IS_NATIVE=1 next build --webpack",
    "build:hosting": "IS_NATIVE=1 next build --webpack",
    "build:firebase": "IS_NATIVE=1 next build --webpack && node scripts/copy-out-to-dist.cjs",
    "deploy": "npm run build:hosting && firebase deploy",
    "deploy:firebase": "npm run build:firebase && firebase deploy",
    "firebase": "npm run build:firebase && firebase deploy",
    "start": "next start",
    "lint": "eslint",
    "setup:env": "cp .env.example .env 2>/dev/null || true",
    "cap:sync": "cap sync",
    "cap:open:ios": "cap open ios",
    "cap": "IS_NATIVE=1 next build --webpack && cap sync && cap open ios"
  },
  "dependencies": {
    "@capacitor-community/media": "^9.0.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/background-runner": "^3.0.0",
    "@capacitor/camera": "^8.0.0",
    "@capacitor/cli": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "@capacitor/file-viewer": "^2.0.1",
    "@capacitor/filesystem": "^8.1.0",
    "@capacitor/geolocation": "^8.0.0",
    "@capacitor/ios": "^8.0.2",
    "@capacitor/preferences": "^8.0.0",
    "@types/leaflet.markercluster": "^1.5.6",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "exifr": "^7.1.3",
    "firebase": "^12.9.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.574.0",
    "next": "16.1.1",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-leaflet": "^5.0.0",
    "react-leaflet-markercluster": "^5.0.0-rc.0",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
